{
    "name": "mrdulal/laravel-vipps",
    "description": "Laravel package for MobilePay Vipps payment integration with ePayment, Checkout, Express, and recurring payments support",
    "type": "library",
    "keywords": ["laravel", "vipps", "mobilepay", "payment", "checkout", "express", "recurring", "subscription"],
    "homepage": "https://github.com/mrdulal/Vipps-MobilePay",
    "license": "MIT",
    "authors": [
        {
            "name": "Mr Dulal",
            "email": "moi@codebundles.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/config": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/validation": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Mrdulal\\LaravelVipps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrdulal\\LaravelVipps\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrdulal\\LaravelVipps\\VippsServiceProvider"
            ],
            "aliases": {
                "Vipps": "Mrdulal\\LaravelVipps\\Facades\\Vipps"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "support": {
        "issues": "https://github.com/mrdulal/Vipps-MobilePay/issues",
        "source": "https://github.com/mrdulal/Vipps-MobilePay"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/mrdulal"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}