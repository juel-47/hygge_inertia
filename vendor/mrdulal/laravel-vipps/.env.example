# Vipps Environment (test or production)
VIPPS_ENVIRONMENT=test

# Vipps API Credentials
# Get these from the Vipps Developer Portal: https://portal.vippsmobilepay.com/
VIPPS_CLIENT_ID=your_client_id_here
VIPPS_CLIENT_SECRET=your_client_secret_here
VIPPS_SUBSCRIPTION_KEY=your_ocp_apim_subscription_key_here
VIPPS_MERCHANT_SERIAL_NUMBER=your_msn_here

# Webhook Configuration
VIPPS_WEBHOOK_SECRET=your_webhook_secret_here

# Logging Configuration
VIPPS_LOGGING_ENABLED=false
VIPPS_LOG_CHANNEL=default
VIPPS_LOG_LEVEL=info